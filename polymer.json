{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-landmarks.html",
    "src/my-memories.html",
    "src/my-reminders.html",
    "src/my-environment.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled",
      "basePath": "/sights-sounds-pwa/"
    },
    {
      "preset": "es6-bundled"
    }
  ]
}
